{
  "name": "node-webkit-angular-app",
  "main": "app://node-webkit-angular-app/index.html",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ank91/node-webkit-angular-starter-kit.git"
  },
  "description": "A sample node-webkit desktop app with angular js",
  "chromium-args": "--disable-setuid-sandbox --disable-accelerated-video --disable-audio --disable-3d-apis --ignore-gpu-blacklist --disable-plugins --disable-plugins-discovery",
  "window": {
    "title": "Node Webkit",
    "icon": "./img/icon.png",
    "toolbar": true,
    "frame": true,
    "width": 1024,
    "height": 768,
    "position": "center",
    "resizable": true,
    "min_width": 640,
    "min_height": 480,
    "max_width": 1024,
    "max_height": 768
  },
  "webkit": {
    "plugin": false,
    "java": false,
    "page-cache": false
  },
  "dependencies": {
  },
  "platformOverrides": {
    "win": {
      "window": {
        "toolbar": false
      }
    },
    "osx": {
      "window": {
        "toolbar": false
      }
    },
    "linux": {
      "window": {
        "toolbar": false
      }
    }
  },
  "manifestUrl": "https://raw.githubusercontent.com/ank91/node-webkit-angular-starter-kit/master/app/package.json",
  "packages": {
    "mac": {
      "url" : ""
    },
    "win": {
      "url" : ""
    },
    "linux32": {
      "url" : ""
    },
    "linux64": {
      "url" : ""
    }
  }
}
